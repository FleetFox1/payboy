#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/payboy/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules/solidity-coverage/plugins/node_modules:/mnt/c/payboy/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules/solidity-coverage/node_modules:/mnt/c/payboy/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules:/mnt/c/payboy/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/payboy/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules/solidity-coverage/plugins/node_modules:/mnt/c/payboy/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules/solidity-coverage/node_modules:/mnt/c/payboy/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules:/mnt/c/payboy/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules/solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/solidity-coverage@0.8.16_ha_b7ca83625e7adcbf6c56ea2455879841/node_modules/solidity-coverage/plugins/bin.js" "$@"
fi
